{
    "name": "padosoft/httpclient",
    "description": "helper for http client guzzle",
    "keywords" : ["httplient"
    ],
    "homepage":"https://github.org/Padosoft/HTTPClient/",
    "license" : "MIT",
    "authors" : [
    	{
    		"name" : "padosoft",
    		"email" : "helpdesk@padosoft.com",
    		"homepage" : "https://www.padosoft.com",
    		"role" : "Developer"
    	}
    ],
	"require": {
        "guzzlehttp/guzzle": "~6.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "phpmd/phpmd": "2.*",
        "squizlabs/php_codesniffer": "2.*",
        "padosoft/test": "0.*"
    },
    "autoload": {
        "psr-4": {
            "Padosoft\\HTTPClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\HTTPClient\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
